@model IEnumerable<E_Commerce.Models.MainMenu>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MainMenuText)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MainMenuText)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MainMenuID }) |
            @Html.ActionLink("Details", "Details", new { id=item.MainMenuID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MainMenuID })
        </td>
    </tr>
}

</table>
